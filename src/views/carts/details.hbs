<div class="main main-raised">
    <h2 class="title">{{ title }}</h2>
    <div class="container">
        <div class="section text-center">
            <h2 class="title"></h2>
            <div class="card-body">
                <div class="card-body">
                    {{!-- <a href="/productshomemg" class="btn btn-info">volver</a> --}}
                </div>
                <table class="table">
                    <thead>
                        <tr>
                            <th class="text-center">#</th>
                            <th>Title</th>
                            <th>Description</th>
                            <th>code</th>
                            <th>price</th>
                            <th>quantity</th>
                            <th>stock</th>
                            <th>category</th>
                            <th class="text-right">Actions</th>
                        </tr>
                    </thead>
                    {{#each cartDetails.products}}
                    <tr>
                        <td class="text-center">{{this.product.id}}</td>
                        <td>{{this.product.title}}</td>
                        <td>{{this.product.description}}</td>
                        <td>{{this.product.code}}</td>
                        <td>{{this.product.price}}</td>
                        <td>{{this.quantity}}</td>
                        <td>{{this.product.stock}}</td>
                        <td>{{this.product.category}}</td>

                       {{!--  <td class="td-actions text-right">
                            <a href="#" type="button" rel="tooltip" title="Add to Cart " class="btn btn-info btn-simple btn-xs">
                                <i class="fa fa-plus"></i>
                            </button>
                            <button type="button" rel="tooltip" title="Edit Product" class="btn btn-success btn-simple btn-xs">
                                <i class="fa fa-edit"></i>
                            </button>
                            <button type="button" rel="tooltip" title="Remove" class="btn btn-danger btn-simple btn-xs">
                                <i class="fa fa-times"></i>
                            </button>
                        </td>  --}}
                    </tr>
                    {{/each}}
                </table>
            </div>
        </div>
    </div>
</div>
